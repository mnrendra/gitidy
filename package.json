{
  "name": "gitidy",
  "version": "0.9.1",
  "description": "A tool for tidying Git flow.",
  "private": false,
  "main": "dist",
  "bin": {
    "gitidy": "dist/index.js"
  },
  "scripts": {
    "build": "rm -rf ./dist && tsc && chmod +x dist/index.js",
    "start": "node .",
    "dev": "npm run build && npm start",
    "link": "npm run build && npm link",
    "test": "exit 0"
  },
  "dependencies": {
    "chalk": "^4.1.2",
    "compressing": "^1.6.3",
    "module-alias": "^2.2.2",
    "rimraf": "^3.0.2"
  },
  "devDependencies": {
    "@types/node": "^18.11.18",
    "@types/rimraf": "^3.0.2",
    "typescript": "^4.9.4"
  },
  "_moduleAliases": {
    "@package.json": "dist/package.json",
    "@core": "dist/core",
    "@cp": "dist/cp",
    "@clog": "dist/clog",
    "@git": "dist/git",
    "@gh": "dist/gh",
    "@lib": "dist/lib"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mnrendra/gitidy.git"
  },
  "keywords": [
    "git"
  ],
  "author": "Muhammad Rendra",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mnrendra/gitidy/issues"
  },
  "homepage": "https://github.com/mnrendra/gitidy#readme"
}
